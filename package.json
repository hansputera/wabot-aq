{
  "name": "wabot-aq",
  "version": "3.0.0",
  "description": "wabot-aq next generation",
  "main": "./index.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint \"**/*.js\" --fix",
    "format": "prettier . --write",
    "build": "rimraf dist && babel src -d dist",
    "watch": "nodemon"
  },
  "keywords": [
    "termux-whatsapp-bot",
    "whatsapp-bot",
    "whatsapp",
    "js-whatsapp"
  ],
  "homepage": "https://github.com/hansputera/wabot-aq",
  "author": {
    "name": "Nurutomo"
  },
  "license": "GPL-3.0-or-later",
  "maintainers": [
    "hansputera"
  ],
  "contributors": [
    "Nurutomo",
    "ariffb25",
    "Ftwrr"
  ],
  "repository": {
    "url": "https://github.com/hansputera/wabot-aq",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/hansputera/wabot-aq/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "eslint": "^8.23.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "gampang": "1.6.7",
    "sharp": "^0.31.0",
    "ytdl-core": "^4.11.2"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/**/*",
      "dist/**/*"
    ],
    "exec": "npm run build && node dist/main.js",
    "watch": [
      "src/commands/**/*"
    ],
    "delay": 10
  }
}
